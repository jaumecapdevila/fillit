{

  "description": "A simple firefox extension to auto fill the forms of a web page",
  "manifest_version": 2,
  "name": "fillit",
  "version": "1.0",
  
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["fillit.js"]
  }],

  "applications": {
    "gecko": {
      "strict_min_version": "58.0"
    }
  }
}
